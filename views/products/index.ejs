<%- include('../partials/header') %>

<h1>All Products</h1>
<div>
  <% for (let product of products) { %>
    <a href="/products/<%= product.id %>">
      <h4><%= product.title %></h4>
    </a>
    <p><%= product.description %></p>
    <p>$<%= product.price %></p>
    <hr/>
  <% } %>
</div>

<%- include('../partials/footer') %>
